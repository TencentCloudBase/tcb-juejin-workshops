
var transform = function (num) {
  switch (num){
      case 0:
      return {
          name:"未支付",
          class:"not"
      }
      case 1:
      return {
          name:"已支付",
          class:"paid"
      }
      case 2:
      return {
          name:"已关闭",
          class:"closed"
      }

  }
}

module.exports.transform = transform;